{
  "openapi": "3.1.0",
  "info": { "title": "FastAPI", "version": "0.1.0" },
  "paths": {
    "/door": {
      "post": {
        "summary": "Coop Door",
        "operationId": "coop_door_door_post",
        "security": [{ "HTTPBearer": [] }],
        "parameters": [
          {
            "name": "direction",
            "in": "query",
            "required": true,
            "schema": { "type": "string", "title": "Direction" }
          }
        ],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": { "$ref": "#/components/schemas/Door" } } }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": { "$ref": "#/components/schemas/HTTPValidationError" }
              }
            }
          }
        }
      }
    },
    "/door/state": {
      "get": {
        "summary": "Door State",
        "operationId": "door_state_door_state_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": { "$ref": "#/components/schemas/Door" } } }
          }
        },
        "security": [{ "HTTPBearer": [] }]
      }
    },
    "/light": {
      "post": {
        "summary": "Lights",
        "operationId": "lights_light_post",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": { "schema": { "$ref": "#/components/schemas/Light" } }
            }
          }
        },
        "security": [{ "HTTPBearer": [] }]
      }
    },
    "/light/state": {
      "get": {
        "summary": "Light State",
        "operationId": "light_state_light_state_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": { "schema": { "$ref": "#/components/schemas/Light" } }
            }
          }
        },
        "security": [{ "HTTPBearer": [] }]
      }
    },
    "/image": {
      "get": {
        "summary": "Latest Image",
        "operationId": "latest_image_image_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": { "application/json": { "schema": {} } }
          }
        },
        "security": [{ "HTTPBearer": [] }]
      },
      "post": {
        "summary": "Capture Image",
        "operationId": "capture_image_image_post",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": { "schema": { "$ref": "#/components/schemas/ImageStatus" } }
            }
          }
        },
        "security": [{ "HTTPBearer": [] }]
      }
    },
    "/temperature": {
      "get": {
        "summary": "Read Temperature",
        "operationId": "read_temperature_temperature_get",
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": { "schema": { "$ref": "#/components/schemas/Temperature" } }
            }
          }
        },
        "security": [{ "HTTPBearer": [] }]
      }
    }
  },
  "components": {
    "schemas": {
      "Door": {
        "properties": { "status": { "$ref": "#/components/schemas/DoorState" } },
        "type": "object",
        "required": ["status"],
        "title": "Door"
      },
      "DoorState": { "type": "integer", "enum": [0, 1, 2, 3, 4], "title": "DoorState" },
      "HTTPValidationError": {
        "properties": {
          "detail": {
            "items": { "$ref": "#/components/schemas/ValidationError" },
            "type": "array",
            "title": "Detail"
          }
        },
        "type": "object",
        "title": "HTTPValidationError"
      },
      "ImageStatus": {
        "properties": {
          "status": { "type": "string", "title": "Status" },
          "filename": { "type": "string", "title": "Filename" }
        },
        "type": "object",
        "required": ["status", "filename"],
        "title": "ImageStatus"
      },
      "Light": {
        "properties": { "status": { "$ref": "#/components/schemas/LightState" } },
        "type": "object",
        "required": ["status"],
        "title": "Light"
      },
      "LightState": { "type": "integer", "enum": [0, 1, 2], "title": "LightState" },
      "Temperature": {
        "properties": {
          "inside": { "type": "number", "title": "Inside" },
          "outside": { "type": "number", "title": "Outside" }
        },
        "type": "object",
        "required": ["inside", "outside"],
        "title": "Temperature"
      },
      "ValidationError": {
        "properties": {
          "loc": {
            "items": { "anyOf": [{ "type": "string" }, { "type": "integer" }] },
            "type": "array",
            "title": "Location"
          },
          "msg": { "type": "string", "title": "Message" },
          "type": { "type": "string", "title": "Error Type" }
        },
        "type": "object",
        "required": ["loc", "msg", "type"],
        "title": "ValidationError"
      }
    },
    "securitySchemes": { "HTTPBearer": { "type": "http", "scheme": "bearer" } }
  }
}
